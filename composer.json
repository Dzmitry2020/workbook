{
  "require": {
    "texlab/mydb": "^0.0.4",
    "ext-mysqli": "*",
    "texlab/html": "^0.05"
  },
  "autoload": {
    "psr-4": {
      "Controller\\": "src/Controller",
      "Core\\": "src/Core",
      "View\\": "src/View",
      "Model\\": "src/Model"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "phpstan/phpstan": "^0.12",
    "vimeo/psalm": "^3.0@dev"
  },
  "minimum-stability": "dev",
  "scripts": {
    "test": "phpunit --testsuite alltests",
    "phpstan": "phpstan analyse -c phpstan.neon",
    "psalm": "psalm"
  }
}
